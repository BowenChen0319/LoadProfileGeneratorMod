<Application
    x:Class="LoadProfileGenerator.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters1="clr-namespace:LoadProfileGenerator.Controls.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\ColorsAndBrushes.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <converters1:PercentConverter x:Key="PercentConverter" />
            <converters1:NumberConverter x:Key="NumberConverter" />
            <converters1:PreciseNumberConverter x:Key="PreciseNumberConverter" />
            <converters1:IntegerConverter x:Key="IntegerConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>